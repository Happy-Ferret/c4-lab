{
    "parserOptions": {
        "ecmaVersion": 6,
        "impliedStrict": true,
        "sourceType": "module"
    },
    "env":{
        "commonjs": true,
        "jasmine": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "eqeqeq": 2,
        "no-extra-parens": 2,
        "arrow-spacing": 2,
        "no-var": 2,
        "prefer-const": 1,
        "max-len": 1,
        "no-alert": 2,
        "no-eval": 2,
        "camelcase": 2
    }
}