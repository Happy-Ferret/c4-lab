{
  "name": "rpd-c4-lab",
  "version": "0.0.0",
  "description": "web interface to create software architecture diagrams",
  "main": "src/index.js",
  "scripts": {
    "test": "karma start karma.conf.js --single-run",
    "test:watch": "karma start karma.conf.js",
    "serve": "live-server src",
    "watch": "watchify src/index.js -d -o src/bundle.js",
    "start": "parallelshell 'npm run watch -s' 'npm run serve -s' 'npm run test:watch'"
  },
  "keywords": [
    "diagrams",
    "software",
    "architecture"
  ],
  "author": "Ryan Davis <ryepup@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.4.8",
    "angular-ui-bootstrap": "^0.14.3",
    "viz.js": "^1.3.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.8",
    "browserify": "^12.0.1",
    "cssify": "^1.0.2",
    "envify": "^3.4.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.16",
    "karma-browserify": "^4.4.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.2",
    "live-server": "^0.9.0",
    "parallelshell": "^2.0.0",
    "phantomjs": "^1.9.19",
    "stringify": "^3.2.0",
    "watchify": "^3.6.1"
  },
  "browserify": {
    "transform": [
      "cssify",
      "stringify",
      "envify"
    ]
  }
}
